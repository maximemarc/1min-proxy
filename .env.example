# 1min.ai API Key (required)
ONEMIN_API_KEY=your_api_key_here

# Server port
PORT=3456

# Environment (development/production)
NODE_ENV=development

# Log level (debug/info/warn/error)
LOG_LEVEL=info

# Rate limiting (15 minutes window, 100 requests max)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# CORS origins (comma-separated, use * for all)
CORS_ORIGINS=*

# 1min.ai API base URL
ONEMIN_BASE_URL=https://api.1min.ai
