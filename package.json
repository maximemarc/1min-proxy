{
  "name": "1min-proxy",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "analyze": "npm run analyze:deps && npm run analyze:circular && npm run analyze:unused",
    "analyze:deps": "depcheck --ignore-bin-package --skip-missing",
    "analyze:circular": "madge --circular src/",
    "analyze:unused": "knip",
    "ci": "npm run typecheck && npm run lint && npm run format:check && npm run test -- --run && npm run analyze"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "pino": "^8.17.2",
    "pino-pretty": "^13.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.3.0",
    "knip": "^5.83.1",
    "madge": "^8.0.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
